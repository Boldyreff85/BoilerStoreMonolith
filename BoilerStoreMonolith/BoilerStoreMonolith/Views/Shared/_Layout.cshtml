<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Boiler Equipment</title>
    <script src="~/Scripts/jquery-3.0.0.js"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>
    <script src="~/Scripts/bootstrap.bundle.js"></script>
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-theme.css" rel="stylesheet" />
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/ErrorStyles.css" rel="stylesheet" type="text/css" />
</head>

<body>
    <div class="container body-content main min-vh-100 px-0">
        @Html.Action("Header")
        @Html.Partial("Menu")
        <div class="row central my-3 mx-auto">
            <div class="col-3">
                @*@Html.Action("CatalogueTree", new { VBCategory = ViewBag.Category })*@
                @Html.Partial("CatalogueTree2")
            </div>
            <div class="col-lg-9">
                @RenderBody()
            </div>
        </div>
        @Html.Action("Footer")
    </div>
</body>
</html>

